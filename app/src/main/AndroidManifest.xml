<?xml version="1.0" encoding="utf-8"?><!-- android:icon="@mipmap/ic_launcher" --><!-- android:roundIcon="@mipmap/ic_launcher_round" -->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.joao.nibe">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.Activity_abertura"
            android:theme="@style/AppTheme.Abertura">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Activity_login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Activity_home" />
        <activity android:name=".activity.Activity_timers"></activity>
    </application>

</manifest>